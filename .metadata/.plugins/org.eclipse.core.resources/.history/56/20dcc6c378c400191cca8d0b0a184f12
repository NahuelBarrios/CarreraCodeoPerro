#include <stdio.h>
#include <stdlib.h>


int main(void) {

	int numeroUno;
	int numeroDos;
	int suma;

	printf("Ingrese el numero uno");
	scanf("%d",&numeroUno);
	printf("Ingrese el numero dos");
	scanf("%d",&numeroDos);
	suma = numeroUno+numeroDos;
	printf("El resultado de la suma es %d",suma);
	return 0;
}
